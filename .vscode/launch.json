{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Pyinfra",
            "type": "python",
            "request": "launch",
            "program": ".venv/bin/pyinfra",
            "cwd": "${workspaceFolder}",
            "args": [
                "pyinfra_test_example.inventories.podman",
                "pyinfra_test_example.main",
                "-y"
            ],
            "env": {
                "GEVENT_SUPPORT": "True"
            },
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
